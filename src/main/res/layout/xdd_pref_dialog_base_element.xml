<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <Button
        android:id="@+id/key"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:enabled="@{viewModel.isUiDifferentFromSharedPref}"
        android:onClick="@{viewModel::resetToSharedPrefValue}"
        android:text="@{viewModel.xddPrefData$XDDLib_debug.key}"
        android:textAllCaps="false" />

    <data>

        <variable
            name="viewModel"
            type="com.example.xddlib.userinput.xddpref.viewmodel.BaseElementVM" />
    </data>
</layout>